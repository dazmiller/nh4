{
    "name": "joomla/cache",
    "type": "joomla-package",
    "description": "Joomla Cache Package",
    "keywords": ["joomla", "framework", "cache"],
    "homepage": "https://github.com/joomla-framework/cache",
    "license": "GPL-2.0+",
    "require": {
        "php": "^5.3.10|~7.0"
    },
    "require-dev": {
        "joomla/test": "~1.0",
        "phpunit/phpunit": "^4.8.35|^5.4.3|~6.0",
        "squizlabs/php_codesniffer": "1.*"
    },
    "conflict": {
        "psr/cache": "*"
    },
    "suggest": {
        "joomla/registry": "Registry can be used as an alternative to using an array for the package options.",
        "ext-apc": "To use APC caching",
        "ext-memcached": "To use Memcached caching",
        "ext-redis": "To use Redis caching",
        "ext-wincache": "To use WinCache caching",
        "ext-xcache": "To use XCache caching"
    },
    "autoload": {
        "psr-0": {
            "Psr": "lib"
        },
        "psr-4": {
            "Joomla\\Cache\\": "src/",
            "Joomla\\Cache\\Tests\\": "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
